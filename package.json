{
  "name": "figlight",
  "version": "0.0.1",
  "description": "Code highlighter for figma",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "convert-styles": "node convert.js",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}'"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "babel-loader": "8.0.6",
    "create-file-webpack": "1.0.2",
    "css-loader": "3.2.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "postcss": "7.0.21",
    "postcss-selector-parser": "6.0.2",
    "prettier": "1.18.2",
    "style-loader": "0.23.1",
    "ts-loader": "6.2.0",
    "tslint": "5.20.0",
    "tslint-react": "4.1.0",
    "typescript": "3.6.4",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "change-case": "3.1.0",
    "color-parse": "1.3.8",
    "cross-storeon": "0.0.1",
    "highlight.js": "9.16.2",
    "lowlight": "1.13.0",
    "storeon": "0.9.5"
  }
}
